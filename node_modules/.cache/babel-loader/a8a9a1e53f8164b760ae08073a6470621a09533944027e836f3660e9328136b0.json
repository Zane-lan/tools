{"ast":null,"code":"import { ref, onMounted, onUnmounted } from 'vue';\nexport default {\n  name: 'TimeTools',\n  setup() {\n    const timestamp = ref(Date.now()); // 初始时间戳\n    let intervalId = null; // 定时器 ID\n\n    // 更新时间戳\n    const updateTimestamp = () => {\n      timestamp.value = Date.now();\n    };\n\n    // 启动定时器\n    onMounted(() => {\n      intervalId = setInterval(updateTimestamp, 100); // 每 100 毫秒更新一次\n    });\n\n    // 清除定时器\n    onUnmounted(() => {\n      if (intervalId) {\n        clearInterval(intervalId);\n      }\n    });\n\n    // 复制时间戳到剪贴板\n    const copyTimestamp = () => {\n      navigator.clipboard.writeText(timestamp.value.toString()).then(() => {\n        alert('时间戳已复制到剪贴板！');\n      }).catch(err => {\n        console.error('复制失败：', err);\n      });\n    };\n    return {\n      timestamp,\n      copyTimestamp\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","name","setup","timestamp","Date","now","intervalId","updateTimestamp","value","setInterval","clearInterval","copyTimestamp","navigator","clipboard","writeText","toString","then","alert","catch","err","console","error"],"sources":["/Volumes/data/project/my_web/tools/src/views/TimeTools.vue"],"sourcesContent":["<template>\n    <div class=\"unix-timestamp\">\n        <p>当前 Unix 时间戳（毫秒）：</p>\n        <p class=\"timestamp\">{{ timestamp }}</p>\n        <button @click=\"copyTimestamp\">复制时间戳</button>\n    </div>\n</template>\n\n<script>\nimport { ref, onMounted, onUnmounted } from 'vue';\n\nexport default {\n    name: 'TimeTools',\n    setup() {\n        const timestamp = ref(Date.now()); // 初始时间戳\n        let intervalId = null; // 定时器 ID\n\n        // 更新时间戳\n        const updateTimestamp = () => {\n            timestamp.value = Date.now();\n        };\n\n        // 启动定时器\n        onMounted(() => {\n            intervalId = setInterval(updateTimestamp, 100); // 每 100 毫秒更新一次\n        });\n\n        // 清除定时器\n        onUnmounted(() => {\n            if (intervalId) {\n                clearInterval(intervalId);\n            }\n        });\n\n        // 复制时间戳到剪贴板\n        const copyTimestamp = () => {\n            navigator.clipboard\n                    .writeText(timestamp.value.toString())\n                    .then(() => {\n                        alert('时间戳已复制到剪贴板！');\n                    })\n                    .catch((err) => {\n                        console.error('复制失败：', err);\n                    });\n        };\n\n        return {\n            timestamp,\n            copyTimestamp,\n        };\n    },\n};\n</script>\n\n<style scoped>\n.unix-timestamp {\n    font-family: Arial, sans-serif;\n    text-align: center;\n    margin: 20px;\n}\n\n.timestamp {\n    font-size: 24px;\n    font-weight: bold;\n    color: #42b983;\n    margin: 10px 0;\n}\n\nbutton {\n    padding: 10px 20px;\n    font-size: 16px;\n    background-color: #42b983;\n    color: white;\n    border: none;\n    border-radius: 4px;\n    cursor: pointer;\n}\n\nbutton:hover {\n    background-color: #369f6e;\n}\n</style>\n"],"mappings":"AASA,SAASA,GAAG,EAAEC,SAAS,EAAEC,WAAU,QAAS,KAAK;AAEjD,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,KAAKA,CAAA,EAAG;IACJ,MAAMC,SAAQ,GAAIL,GAAG,CAACM,IAAI,CAACC,GAAG,CAAC,CAAC,CAAC,EAAE;IACnC,IAAIC,UAAS,GAAI,IAAI,EAAE;;IAEvB;IACA,MAAMC,eAAc,GAAIA,CAAA,KAAM;MAC1BJ,SAAS,CAACK,KAAI,GAAIJ,IAAI,CAACC,GAAG,CAAC,CAAC;IAChC,CAAC;;IAED;IACAN,SAAS,CAAC,MAAM;MACZO,UAAS,GAAIG,WAAW,CAACF,eAAe,EAAE,GAAG,CAAC,EAAE;IACpD,CAAC,CAAC;;IAEF;IACAP,WAAW,CAAC,MAAM;MACd,IAAIM,UAAU,EAAE;QACZI,aAAa,CAACJ,UAAU,CAAC;MAC7B;IACJ,CAAC,CAAC;;IAEF;IACA,MAAMK,aAAY,GAAIA,CAAA,KAAM;MACxBC,SAAS,CAACC,SAAQ,CACTC,SAAS,CAACX,SAAS,CAACK,KAAK,CAACO,QAAQ,CAAC,CAAC,EACpCC,IAAI,CAAC,MAAM;QACRC,KAAK,CAAC,aAAa,CAAC;MACxB,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAK;QACZC,OAAO,CAACC,KAAK,CAAC,OAAO,EAAEF,GAAG,CAAC;MAC/B,CAAC,CAAC;IACd,CAAC;IAED,OAAO;MACHhB,SAAS;MACTQ;IACJ,CAAC;EACL;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}